<idea-plugin>
    <id>org.example.google_search_plugin</id>
    <name>Error google search</name>
    <vendor email="support@mycompany.com" url="http://www.mycompany.com">MyCompany</vendor>

    <description>Google search if error occurs. Find error description.</description>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <action
            id="SearchErrorAction"
            class="SearchErrorAction"
            text="Stackoverflow search"
            description="Stackoverflow search what you want">
            <add-to-group group-id="ConsoleEditorPopupMenu" anchor="first"/>
        </action>
    </actions>
</idea-plugin>